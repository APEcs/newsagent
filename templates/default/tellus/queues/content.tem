<script type="text/javascript">
var mlisturl = "***mlist-url***";
var selects;

window.addEvent('domready', function() {
    $$("table.listtable li.info a").each(function(elem) {
        elem.addEvent("click", function(event) { event.stopPropagation(); });
    });
    selects = new SelectControl('select-ctrl');
});
</script>
<div id="queuelist">
    <div class="shadowbox">
        <h2>{L_TELLUS_QLIST_TITLE}</h2>
        <div class="sbcontent">
            <div id="messagebrowser">
                <div id="msgops">
                    <div id="select-ctrl" class="selctrl" style="float: left">
                        <span class="selctrl-mode">&nbsp;</span>
                        <span class="selctrl-arr">&nbsp;</span>
