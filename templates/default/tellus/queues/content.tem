<div id="queuelist" class="shadowbox">
    <h2>{L_TELLUS_QUEUES_TITLE}</h2>
    <div class="sbcontent">
        <div id="queuebrowser">
            <h3>{L_TELLUS_QUEUELIST}</h3>
            <table class="queues">
                <thead>
                    <tr><th>{L_TELLUS_QUEUE_NAME}</th>
                        <th><img src="src="{V_[templatepath]}images/tellus/queuenew.png"  width="16" height="16" title="{L_TELLUS_QUEUE_NEW}" /></th>
                        <th><img src="src="{V_[templatepath]}images/tellus/queueread.png" width="16" height="16" title="{L_TELLUS_QUEUE_READ}" /></th>
                        <th><img src="src="{V_[templatepath]}images/tellus/queueall.png"  width="16" height="16" title="{L_TELLUS_QUEUE_ALL}" /></th>
                    </tr>
                </thead>
                <tbody>
***queues***
                </tbody>
            </table>
        </div>
        <div id="messagebrowser">
            <h3>{L_TELLUS_MESSAGELIST}</h3>
            <table class="listtable">
                <tbody>
***messages***
                </tbody>
            </table>
***paginate***
        </div>
    </div>
</div>
