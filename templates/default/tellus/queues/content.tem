<script type="text/javascript">
mlisturl       = "***mlist-url***";
composeurl     = "***compose-url***";
spinner_imgurl = '{V_[templatepath]}images/spinner.gif';
messages = { "promote": "{L_TELLUS_QLIST_MAKEARTICLE}",
              "reject": "{L_TELLUS_QLIST_REJECT}",
            "rejtitle": "{L_TELLUS_QLIST_REJTITLE}",
              "delete": "{L_TELLUS_QLIST_DELETE}",
              "cancel": "{L_TELLUS_QLIST_POP_CANCEL}",
                "view": "{L_TELLUS_QLIST_MSGVIEW}"
};
</script>
<div id="queuelist">
    <div class="shadowbox">
        <h2>{L_TELLUS_QLIST_TITLE}</h2>
        <div class="sbcontent">
            <div id="messagebrowser">
                <div id="msgops">
                    <div id="select-ctrl" class="selctrl" style="float: left">
                        <span class="selctrl-mode">&nbsp;</span>
                        <span class="selctrl-arr">&nbsp;</span>
                        <ul>
                            <li class="selctrl-opt" data-selctrl-mode="all">{L_TELLUS_QLIST_SELALL}</li><!--
                         --><li class="selctrl-opt" data-selctrl-mode="none">{L_TELLUS_QLIST_SELNONE}</li><!--
                         --><li class="selctrl-opt" data-selctrl-mode="new">{L_TELLUS_QLIST_SELNEW}</li><!--
                         --><li class="selctrl-opt" data-selctrl-mode="read">{L_TELLUS_QLIST_SELREAD}</li>
                        </ul>
                    </div>
                    <div id="controls-box">
                        <img id="movespin" src="{V_[templatepath]}images/spinner.gif" width="16" height="16" alt="working" class="workspin" />
                        <ul id="message-controls" class="msgctrl">
                            ***targets***<li class="msgctrl-reject" title="{L_TELLUS_QLIST_REJECT}"><img src="{V_[templatepath]}images/tellus/reject.png" width="16" height="16" alt="reject" /></li><li class="msgctrl-delete" title="{L_TELLUS_QLIST_DELETE}"><img src="{V_[templatepath]}images/tellus/delete.png" width="16" height="16" alt="delete" /></li>
                        </ul>
                    </div>
                </div>
                <table class="listtable">
                   <tbody>
***messages***
                   </tbody>
                </table>
***paginate***
            </div>
            <div id="queuebrowser">
***queues***
            </div>
            <div class="clear"></div>
        </div>
    </div>
</div>
